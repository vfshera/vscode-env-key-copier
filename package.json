{
  "icon": "assets/icon.png",
  "name": "env-key-copier",
  "displayName": "Env Key Copier",
  "description": "Copy keys or key=value pairs from .env files",
  "publisher": "FranklinShera",
  "author": {
    "name": "Franklin Shera"
  },
  "homepage": "https://github.com/vfshera/vscode-env-key-copier",
  "bugs": "https://github.com/vfshera/vscode-env-key-copier/issues",
  "repository": {
    "url": "https://github.com/vfshera/vscode-env-key-copier",
    "type": "git"
  },
  "keywords": [
    "vscode",
    "env",
    "key"
  ],
  "version": "1.2.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.100.0"
  },
  "packageManager": "pnpm@10.9.0",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dotenv",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "envKeyCopier.copyKeys",
        "title": "Copy ENV Keys"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "envKeyCopier.copyKeys",
          "when": "resourceFilename =~ /^\\.env(\\..+)?$/",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.mjs",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.mjs --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.mjs --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "check-types": "tsc --noEmit",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "release": "npx @vscode/vsce publish && pnpm changeset tag"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "typescript": "^5.8.3",
    "esbuild": "^0.25.3",
    "npm-run-all": "^4.1.5"
  }
}
